{
  "name": "golfing-grouper",
  "version": "1.0.0",
  "description": "A standalone static website that serves as a wrapper around multiple golf booking platforms",
  "main": "src/server.js",
  "scripts": {
    "start": "node src/server.js",
    "dev": "nodemon src/server.js",
    "client": "cd client && npm start",
    "dev:full": "concurrently \"npm run dev\" \"npm run client\"",
    "db:setup": "node src/db/setup.js",
    "scraper": "node src/workers/scraperWorker.js",
    "build": "cd client && npm run build",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:unit": "jest --testPathPattern=__tests__/unit",
    "test:controllers": "jest --testPathPattern=__tests__/unit/controllers",
    "test:services": "jest --testPathPattern=__tests__/unit/services",
    "test:middleware": "jest --testPathPattern=__tests__/unit/middleware",
    "docker:build": "docker compose build",
    "docker:up": "docker compose up",
    "docker:down": "docker compose down",
    "docker:dev": "docker compose -f docker-compose.yml -f docker-compose.dev.yml up",
    "docker:test": "docker compose -f docker-compose.test.yml up --abort-on-container-exit",
    "docker:clean": "docker compose down -v && docker compose -f docker-compose.test.yml down -v"
  },
  "keywords": [
    "golf",
    "booking",
    "matching",
    "tee-times"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@sendgrid/mail": "^8.1.0",
    "axios": "^1.6.2",
    "bcrypt": "^5.1.1",
    "cheerio": "^1.0.0-rc.12",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "node-cron": "^3.0.3",
    "pg": "^8.11.3",
    "pg-hstore": "^2.3.4",
    "puppeteer": "^22.8.2",
    "sequelize": "^6.35.2",
    "stripe": "^14.11.0",
    "uuid": "^9.0.1",
    "validator": "^13.11.0"
  },
  "devDependencies": {
    "concurrently": "^8.2.2",
    "jest": "^29.7.0",
    "nodemon": "^3.0.2",
    "sequelize-cli": "^6.6.2",
    "supertest": "^6.3.3"
  }
} 